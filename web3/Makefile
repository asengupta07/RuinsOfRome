compile:
	npx hardhat compile

deploy_Celestial:
	npx hardhat run ignition/modules/celestial.ts --network fuji

deploy_Gladiator:
	npx hardhat run ignition/modules/gladiator.ts --network fuji

verify_Celestial:
	npx hardhat verify --network fuji 0x6D4dc646E54Fd51B7d16DCdd341eAB12d6FE6916

verify_Gladiator:
	npx hardhat verify --network fuji 0xcd6797834d00fc0Abd1E1B6c647983C38e69D3F4