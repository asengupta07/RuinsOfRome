compile:
	npx hardhat compile

deploy_Celestial:
	npx hardhat run ignition/modules/celestial.ts --network fuji

deploy_Gladiator:
	npx hardhat run ignition/modules/gladiator.ts --network fuji

verify_Celestial:
	npx hardhat verify --network fuji 0xCf742f087858F739Fa1a4Db6f17c17531D1Ab980

verify_Gladiator:
	npx hardhat verify --network fuji 0xf17Bb42125b8059B41f0e8f3AFC84fa10c86021f